package practiceTest;

import java.time.Duration;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.Test;




public class trym   {
	
	@Test
	public void test() {
		
		WebDriver driver=new ChromeDriver();
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(20));
		driver.get("https://www.ebay.com/?msockid=35a2da988e646c7b1238ce748f1b6d1f");
		
		driver.findElement(By.id("gh-ac")).sendKeys("kill la kill",Keys.ENTER);
		
		driver.findElement(By.xpath("//span[text()='Nintendo Switch Kill La Kill The Game IF Limited Japanese Edition CIB ']")).click();
		
		
	}
}
